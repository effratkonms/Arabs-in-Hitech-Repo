{
  "$schema": "https://developer.microsoft.com/json-schemas/fabric/item/report/definition/page/1.0.0/schema.json",
  "name": "ReportSection",
  "displayName": "בקרה",
  "displayOption": "FitToWidth",
  "height": 720,
  "width": 1280,
  "visibility": "HiddenInViewMode",
  "objects": {
    "background": [
      {
        "properties": {
          "color": {
            "solid": {
              "color": {
                "expr": {
                  "ThemeDataColor": {
                    "ColorId": 0,
                    "Percent": -0.1
                  }
                }
              }
            }
          },
          "transparency": {
            "expr": {
              "Literal": {
                "Value": "100D"
              }
            }
          }
        }
      }
    ],
    "outspace": [
      {
        "properties": {
          "color": {
            "solid": {
              "color": {
                "expr": {
                  "ThemeDataColor": {
                    "ColorId": 0,
                    "Percent": -0.1
                  }
                }
              }
            }
          }
        }
      }
    ]
  }
}